<nav>
    <h2><a href="index.html">Index</a></h2>
    <div id="navTree"></div>
    <script>
        $(function() {
            var tree = $('#navTree');
            // generate the collapsible tree.
            tree.tree({
                data: <?js= JSON.stringify(this.nav) ?>,
                saveState: 'jsdoc-navtree',
                autoEscape: false
            });
            var current = tree.tree('getNodeById', '<?js= filename ?>');
            // expand the node for the current. If it's a leaf node we have to expand the parent.
            if (current) {
                // TODO: this should not be saved (but manual changes should).
                if (current.isFolder()) {
                    tree.tree('openNode', current, false);
                } else if (current.parent) {
                    tree.tree('openNode', current.parent, false);
                }
                // add id="current-page" to that node.
                current.element.id = 'current-page';
            }
        });
    </script>
</nav>

